{
  "name": "docopt",
  "version": "1.0.0",
  "description": "command line option parser, that will make you smile",
  "main": "commonjs/docopt.cjs",
  "directories": {
    "example": "examples",
    "test": "test",
    "lib": "src"
  },
  "files": [
    "commonjs/**/*",
    "esm/**/*",
    "examples/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^13.7.1",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=10.3.0"
  },
  "scripts": {
    "test": "node test/test.mjs",
    "compileEsm": "tsc",
    "compileCommon": "tsc -b tsconfig.commonjs.json && mv commonjs/docopt.js commonjs/docopt.cjs",
    "prepublish": "npm run compileCommon && npm run compileEsm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eyal-Shalev/docopt.js.git"
  },
  "keywords": [
    "docopt",
    "cli",
    "options"
  ],
  "author": "Eyal Shalev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eyal-Shalev/docopt.js/issues"
  },
  "homepage": "https://github.com/Eyal-Shalev/docopt.js#readme"
}
